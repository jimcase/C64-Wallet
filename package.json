{
  "name": "c64-minter",
  "version": "1.0.0",
  "description": "An Electron application with React and Node",
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run server-start\" \"wait-on http://localhost:3000 && npm run electron-dev\"",
    "react-build": "react-app-rewired build",
    "react-start": "BROWSER=none react-app-rewired start",
    "server-build": "babel server --out-dir build-server",
    "server-start": "nodemon --watch server --exec babel-node server/server.js",
    "electron-dev": "node scripts/dev.js scripts/start.js",
    "electron-pack": "electron-builder -c.extraMetadata.main='public/electron.js'",
    "preelectron-pack": "npm run react-build && npm run server-build"
  },
  "author": "tbuyse",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.4",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "concurrently": "^4.1.0",
    "customize-cra": "^0.2.12",
    "electron": "^12.0.2",
    "electron-builder": "^20.41.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.2.0",
    "moment-range": "^4.0.2",
    "react-app-rewired": "^2.1.3",
    "react-scripts": "3.0.1",
    "typescript": "^4.2.4",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@babel/helper-define-map": "^7.13.12",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@zip.js/zip.js": "^2.2.27",
    "antd": "^4.15.3",
    "app-module-path": "^2.2.0",
    "axios": "^0.21.1",
    "bourbon": "^7.0.0",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "mauerwerk": "^2.0.2",
    "mime-types": "^2.1.30",
    "moment": "^2.29.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "pako": "^1.0.10",
    "parse-uri": "^1.0.3",
    "piping": "^1.0.0-rc.4",
    "react": "^16.14.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.14.0",
    "react-icons": "^4.2.0",
    "react-image-magnifiers": "^1.4.0",
    "react-konva": "^17.0.2-0",
    "react-magnifier": "^3.0.4",
    "react-router-dom": "^5.2.0"
  },
  "build": {
    "appId": "com.electron-react-node",
    "files": [
      "build/**/**/*",
      "build-server/**/**/*",
      "node_modules/**/*",
      "public/electron.js"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "browserslist": []
}
